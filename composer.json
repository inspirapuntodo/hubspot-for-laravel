{
    "name": "inspirapuntodo/hubspot-for-laravel",
    "description": "Hubspot for Laravel allows you to interact with the official hubspot PHP Api Client",
    "keywords": ["laravel", "php", "sdk", "hubspot", "api"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "InspiraPuntoDo\\Hubspot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Cesar Mendez",
            "email": "cesar@inspira.do"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1.0",
        "laravel/framework": "^9.52.0|^10.2.0",
        "hubspot/hubspot-php": "^5.0.0"
    },
    "require-dev": {
        "laravel/pint": "^1.3",
        "pestphp/pest": "^2.0.0",
        "phpstan/phpstan": "^1.9.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "InspiraPuntoDo\\Hubspot\\ServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
